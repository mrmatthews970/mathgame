    "forbidden = \"qwertyuiopasdfghjklzxcvbnm+*/()^' \"\n",
    "def check_op(op,a,b):\n",
    "    if op == \"add\":\n",
    "        value = a+b\n",
    "        answer = input(\"What is \"+str(a)+\" plus \"+str(b)+\"? \")\n",
    "    elif op == \"subtract\":\n",
    "        value = a-b\n",
    "        answer = input(\"What is \"+str(a)+\" minus \"+str(b)+\"? \")\n",
    "    elif op == \"multiply\":\n",
    "        value = a*b\n",
    "        answer = input(\"What is \"+str(a)+\" times \"+str(b)+\"? \")\n",
    "    elif op == \"divide\":\n",
    "        c = choice([a,b])\n",
    "        value = (a*b)//c\n",
    "        answer = input(\"What is \"+str(a*b)+\" divided by \"+str(c)+\"? \")\n",
    "    elif op == \"exp\":\n",
    "        value = pow(a,b)\n",
    "        answer = input(\"What is \"+str(a)+\" to the power of \"+str(b)+\"? \")\n",
    "    elif op == \"sqrt\":\n",
    "        value = np.sqrt(a**2)\n",
    "        answer = input(\"What is the square root of \"+str(a**2)+\"? \")\n",
    "    elif op == \"cbrt\":\n",
    "        value = np.cbrt(a**3)\n",
    "        answer = input(\"What is the cube root of \"+str(a**3)+\"? \")\n",
    "    elif op == \"4thrt\":\n",
    "        value = np.sqrt(np.sqrt(a**4))\n",
    "        answer = input(\"What is the fourth root of \"+str(a**4)+\"? \")\n",
    "        \n",
    "    if \"-\" in answer:\n",
    "        if answer.find(\"-\") != 0 or len(answer) == 1:\n",
    "            return \"Incorrect\"\n",
    "    for char in answer:\n",
    "        if char in forbidden:\n",
    "            return \"Incorrect!\"\n",
    "    if len(answer) == 0:\n",
    "        return \"Incorrect!\"\n",
    "    else:\n",
    "        answer = float(answer)\n",
    "        if abs(value - answer) < 1e-10:\n",
    "            return \"Correct!\"\n",
    "        else:\n",
    "            return \"Incorrect!\""
   ]
